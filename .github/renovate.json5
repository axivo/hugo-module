{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": [
    "fmunteanu"
  ],
  "customDatasources": {
    "cilium-cli": {
      "defaultRegistryUrlTemplate": "https://github.com/cilium/cilium-cli/blob/main/stable.txt",
      "format": "html"
    }
  },
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "custom.cilium-cli",
      "depNameTemplate": "cilium/cilium-cli",
      "fileMatch": [
        "config.yaml"
      ],
      "matchStrings": [
        "release:[\\s\\S]+cli:[\\s\\S]+?version:\\s+(?<currentValue>.+)"
      ],
      "versioningTemplate": "semver-coerced"
    },
    {
      "customType": "regex",
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "golang/go",
      "extractVersionTemplate": "^go(?<version>.+)$",
      "fileMatch": [
        "netlify.toml"
      ],
      "matchStrings": [
        "^GO_VERSION[\\s\\S]+?\"(?<currentValue>.+)\""
      ]
    }
  ],
  "enabledManagers": [
    "custom.regex",
    "gomod"
  ],
  "extends": [
    "config:recommended"
  ],
  "labels": [
    "dependency"
  ],
  "packageRules": [
    {
      "groupName": "dependency golang/go",
      "matchDatasources": [
        "github-tags"
      ],
      "matchManagers": [
        "custom.regex"
      ],
      "matchPackagePrefixes": [
        "golang/"
      ],
      "matchUpdateTypes": [
        "major",
        "minor"
      ]
    },
    {
      "groupName": "dependency golang/go",
      "matchDatasources": [
        "golang-version"
      ],
      "matchManagers": [
        "gomod"
      ],
      "matchUpdateTypes": [
        "major",
        "minor"
      ]
    },
    {
      "enabled": true,
      "matchDepTypes": [
        "indirect"
      ],
      "matchManagers": [
        "gomod"
      ]
    }
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0
}

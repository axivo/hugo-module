{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": [
    "fmunteanu"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "golang/go",
      "extractVersionTemplate": "^go(?<version>.+)$",
      "fileMatch": [
        "netlify.toml"
      ],
      "matchStrings": [
        "^GO_VERSION[\\s\\S]+?\"(?<currentValue>.+)\""
      ]
    }
  ],
  "extends": [
    "config:recommended"
  ],
  "labels": [
    "dependency"
  ],
  "packageRules": [
    {
      "groupName": "dependency golang/go",
      "matchDatasources": [
        "github-tags"
      ],
      "matchManagers": [
        "custom.regex"
      ],
      "matchPackagePrefixes": [
        "golang/"
      ],
      "matchUpdateTypes": [
        "major",
        "minor"
      ]
    },
    {
      "groupName": "dependency golang/go",
      "matchDatasources": [
        "golang-version"
      ],
      "matchManagers": [
        "gomod"
      ],
      "matchUpdateTypes": [
        "major",
        "minor"
      ]
    },
    {
      "enabled": true,
      "matchDepTypes": [
        "indirect"
      ],
      "matchManagers": [
        "gomod"
      ]
    }
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0
}
